		<dependency>
			<groupId>j2ee</groupId>
			<artifactId>jsp-api</artifactId>
			<version>2.0</version>
			<type>jar</type>
			<properties>
				<war.manifest.classpath>true</war.manifest.classpath>
				<war.bundle>false</war.bundle>
			</properties>
		</dependency>		 
